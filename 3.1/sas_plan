(fill r1 b1 food depot)
(pickup r1 b1 depot)
(move_with_box r1 depot loc1 b1)
(unload r1 b1 loc1)
(empty_box r1 b1 food loc1)
(serve_people food loc1 p1)
(fill r1 b1 food loc1)
(pickup r1 b1 loc1)
(move_with_box r1 loc1 loc2 b1)
(unload r1 b1 loc2)
(empty_box r1 b1 food loc2)
(serve_people food loc2 p2)
; cost = 12 (unit cost)
