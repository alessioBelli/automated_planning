(load r1 b1 c1 depot capacity_3 capacity_4)
(fill r1 b4 apple depot)
(load r1 b4 c1 depot capacity_2 capacity_3)
(fill r1 b3 banana depot)
(move_carrier r1 c1 depot loc1)
(unload r1 b4 c1 loc1 capacity_2 capacity_3)
(empty_box r1 b4 apple loc1)
(move_carrier r1 c1 loc1 loc2)
(unload r1 b1 c1 loc2 capacity_3 capacity_4)
(serve_person apple loc1 alessio)
(return_to_depot r1 c1 loc2 depot capacity_4 capacity_0)
(load r1 b3 c1 depot capacity_3 capacity_4)
(fill r1 b2 hammer depot)
(move_carrier r1 c1 depot loc1)
(unload r1 b3 c1 loc1 capacity_3 capacity_4)
(empty_box r1 b3 banana loc1)
(move_carrier r1 c1 loc1 loc2)
(return_to_depot r1 c1 loc2 depot capacity_4 capacity_0)
(serve_person banana loc1 alberto)
(load r1 b2 c1 depot capacity_3 capacity_4)
(move_carrier r1 c1 depot loc2)
(unload r1 b2 c1 loc2 capacity_3 capacity_4)
(empty_box r1 b2 hammer loc2)
(serve_person hammer loc2 stefano)
; cost = 24 (unit cost)
